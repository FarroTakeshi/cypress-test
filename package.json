{
  "name": "cypress-test",
  "version": "1.0.0",
  "description": "",
  "homepage": "https://github.com/FarroTakeshi/cypress-test#readme",
  "bugs": {
    "url": "https://github.com/FarroTakeshi/cypress-test/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FarroTakeshi/cypress-test.git"
  },
  "license": "ISC",
  "author": "tfarrohi",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cy:open": "cypress open",
    "cy:run-default": "cypress run --spec cypress/e2e/todo_spec.cy.js",
    "cy:run-headless": "cypress run --headless --spec cypress/e2e/todo_spec.cy.js",
    "cy:run-cloud": "cypress run --record --key 06615e17-42d8-4003-a10b-5a0d69d299ff",
    "cy:run:cucumber": "cypress run --headless --spec cypress/e2e/login/swag_login.feature",
    "cy:run:smoke-tests": "cypress run --record --key 06615e17-42d8-4003-a10b-5a0d69d299ff --env tags=@smokeTest"
  },
  "devDependencies": {
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.5",
    "cypress": "^14.3.3",
    "esbuild": "^0.25.4"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1"
  },
  "stepDefinitions": [
    "cypress/e2e/[filepath]/**/*.{js,ts}",
    "cypress/e2e/[filepath].{js,ts}",
    "cypress/support/step_definitions/**/*.{js,ts}"
  ],
  "cypress-cucumber-preprocessor": {
    "html": {
      "enabled": false
    }
  }
}
